## -*- Mode: python; py-indent-offset: 4; indent-tabs-mode: nil; coding: utf-8; -*-

def build(bld):
    obj = bld.create_ns3_program('video-streaming', ['internet', 'csma', 'video-push', 'point-to-point', 'applications'])
    obj.source = 'video-streaming.cc'
    obj = bld.create_ns3_program('streaming-pull', ['internet', 'csma', 'video-push', 'wifi', 'aodv', 'applications', 'visualizer'])
    obj.source = 'streaming-pull.cc'